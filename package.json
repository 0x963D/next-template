{
  "name": "artdevium-next-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier -w \"{src,pages}/**/*.{ts,tsx,css,js}\"",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prisma-studio": "prisma generate && prisma studio",
    "prisma-migrate": "prisma migrate dev --name \"localhost\""
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-slot": "^1.0.2",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "0.105.0-alpha.4",
    "next": "^14.0.0",
    "next-themes": "^0.2.1",
    "prisma": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.4",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.8.7",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18",
    "@types/react-modal": "^3.16.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
